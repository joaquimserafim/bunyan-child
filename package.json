{
  "name": "bunyan-child",
  "version": "0.0.0",
  "description": "create a primary instance of a bunyan logger and many `child` instances from the primary and with support to send the logs to logstash",
  "main": "index.js",
  "files": [
    "LICENSE",
    "README.md",
    "index.js"
  ],
  "scripts": {
    "test": "lab -v -c test.js",
    "check-coverage": "lab -t 100 -r html -o coverage.html test.js",
    "open-coverage": "open coverage.html",
    "jshint": "jshint -c .jshintrc *.js",
    "code-style": "jscs -p google *.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joaquimserafim/bunyan-child.git"
  },
  "keywords": [
    "logging",
    "log",
    "bunyan",
    "logstash",
    "elasticsearch",
    "elk",
    "kibana",
    "utilities"
  ],
  "author": "@joaquimserafim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaquimserafim/bunyan-childr/issues"
  },
  "homepage": "https://github.com/joaquimserafim/bunyan-child",
  "pre-commit": [
    "jshint",
    "code-style",
    "test",
    "check-coverage"
  ],
  "dependencies": {
    "bunyan": "^1.4.0",
    "bunyan-logstash": "^0.3.4"
  },
  "devDependencies": {
    "code": "^1.4.1",
    "jscs": "^1.13.1",
    "jshint": "^2.8.0",
    "lab": "^5.13.0",
    "pre-commit": "^1.0.10"
  }
}
